	//编辑tag标签
	function showEditInput(obj,serviceId)
	{
		var tagValue = $(obj).text();
		var tagId = $(obj).attr('id');
		tagId=tagId.replace("edit_","");
		var inputStr = "<textarea  class=\"span-13\" name=\"tag\" id="+tagId+" >"+tagValue+"</textarea> <button onclick=\"saveTagBtn("+tagId+")\" class=\"normal right\">保&nbsp;存</button>  <button onclick=\"cancle("+tagId+")\" class=\"normal right\">取&nbsp;消</button>";
		$(obj).html(inputStr);
	}
	
	function showEditInputId(serviceId)
	{
		var tagValue = $('#edit_'+serviceId+'').text();
		var tagId = $('#edit_'+serviceId+'').attr('id');
		tagId=tagId.replace("edit_","");
		var inputStr = "<textarea  class=\"span-13\" name=\"tag\" id="+tagId+" >"+tagValue+"</textarea> <button onclick=\"saveTagBtn("+tagId+")\" class=\"normal right\">保&nbsp;存</button>  <button onclick=\"cancle("+tagId+")\" class=\"normal right\">取&nbsp;消</button>";
		$('#edit_'+serviceId+'').html(inputStr);
	}
	
	
	//异步保存
	function saveTagBtn(id)
	{
		var tagValue = $('#'+id+'').val();
		var tagId = id;
		var qId = $('#questionid').val();
		$.ajax({
			type:"GET",
			url:"/doEditAnswer.html",
			data:"id="+tagId+"&qid="+qId+"&answer="+tagValue,
			success:function(data){
				if(data.search(/filter/i)!=-1){
					jQuery.noticeAdd({text: '不能包含敏感词汇',stay: false});
				}else{
					$('#'+id+'').parent().text(tagValue);//恢复为文本形式
					$('#old_'+tagId+'').text(tagValue);
				}
			}
		});
		
	}
	
	
	function cancle(id)
	{
		var tagValue = $('#old_'+id+'').text();
		$('#'+id+'').parent().text(tagValue);//恢复为文本形式
	}	
	
</script>
